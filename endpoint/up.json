{
  "name": "wsburroughs-link-ldp",
  "proxy": {
    "command": "FLASK_APP=app.py python -m flask run -p $PORT"
    },
  "stages": {
    "production": {
      "domain": "wsburroughs.link",
      "lambda": {
          "memory": 128
      },
      "proxy": {
          "timeout": 25
      }
    },
    "staging": {
      "domain": "dev.wsburroughs.link",
      "lambda": {
          "memory": 128
      },
      "proxy": {
          "timeout": 25
      }
    }
  }
}
